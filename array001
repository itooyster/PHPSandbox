How much is 2*3?
6
How much is 3+5?
8
How much is 6-1?
5

<?php

$quiz = [
'How much is 2*3?',
'6',
'How much is 3+5?',
'8',
'How much is 6-1?',
'5'
];

echo $quiz;
echo '<hr>';
echo print_r($quiz);
echo '<hr>';
echo "<pre>",print_r($quiz),"</pre>";
echo '<hr>';
echo $quiz[0];
echo '<hr>';
$y=1;
echo $quiz[$y];
echo '<hr>';

foreach($quiz as $x) {
  echo $x . "<br>";
}
?>
